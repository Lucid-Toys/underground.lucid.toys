  "author": "Daniel Eden <dan.eden@me.com>",
  "license": "MIT",
  "dependencies": {
    "@cypress/code-coverage": "^3.9.2",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "global": "^4.4.0",
