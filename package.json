    "next": "^10.0.5",
    "next-images": "^1.4.1",
    "next-pwa": "^5.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "swr": "^0.4.0",
    "unfetch": "^4.1.0"
  },
  "scripts": {
    "dev": "next -p 7000",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "npm-run-all lint:*",
    "precommit": "lint-staged",
